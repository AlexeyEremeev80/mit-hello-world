#include <iostream>

using namespace std;

int Fibb(int x) {
	if (x <= 1) return 0;
	if (x == 2) return 1;
	if (x > 2) return Fibb(x - 1) + Fibb(x - 2);
}


int main(){
	setlocale(LC_ALL, "RUS");
	int num_func_Fibb;
	cout << "Введите номер последовательности чисел Фиббоначи: ";
	cin >> num_func_Fibb;
	cout << Fibb(num_func_Fibb);
	return 0;
}	